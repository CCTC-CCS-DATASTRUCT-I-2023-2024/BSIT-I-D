#include <iostream>
using namespace std;

int main() {
    int a = 10;
    int b = 20;
    int c = 40;
    
    int sum = a + b + c;
    int diff = a - b - c;
    int prod = a * b * c;
    int quot = a / b / c;
    
    cout<< "Sum is = "<<sum << endl;
    cout<< "Difference is = "<<diff << endl;
    cout<< "Product is = "<<prod << endl;
    cout<< "Quotient is = "<<quot << endl;
}